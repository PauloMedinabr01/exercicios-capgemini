Algoritmo "Escolha"





Var

   voto : caractere

Inicio

   escreval("Digite seu voto: ")
   leia(voto)

   escolha voto
   caso "20"
      escreval("Você votou em Fulano.")
   caso "30"
      escreval("Você votou em Beltrano.")
   caso "40"
      escreval("Você votou em Ciclano.")
   outrocaso
   escreval ("Voto nulo.")

Fimalgoritmo